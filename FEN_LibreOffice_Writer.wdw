          name : BTN_Publipostage
          identifier : 0x2f77249906225358
          internal_properties : 
          properties :
           x : 374
           y : 180
           width : 112
           height : 24
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              internal_properties : CAAAAAgAAAAXztX2MB1lOhtfm9GIe8Ds2SGkgfUf+xhLFC5PJ2Eeeo/RoSa/lqTwomCiTrm3fRffAHDp1ulKbw2YuLyr9JgAHujOEqEIjQaNdrDx4RdKINqHLvI9W6znuZ6KMs8RyPDMVncoRxkIUSEDrcTmByUTCCaMjCcyW9/mA9lM4sE6WM9H2yYHt1i59mX8Hv6ROPs=
              code : |1+
               
               // Si un document est déjà ouvert
               SI gclDocumentWriter:EstOuvert() ALORS
               	
               	SELON Confirmer("Un document est déjà ouvert.","Voulez-vous le fermer pour lancer le publipostage ?")		
               		CAS Oui
               			//Fermeture du document
               			gclDocumentWriter:FermerDocument()
               		CAS Non
               			//Le document ouvert ne sera plus manipulable
               			Info("Si vous réouvrez un document déjà actif, il sera en lecture seule")			
               		CAS Annuler
               			Sablier(Faux)
               			RETOUR
               	FIN
               FIN
               
               sDocument = sai_doctype
               
               SI sDocument <> "" ALORS
               	
               	// Ouvrir un document
               	SI gclDocumentWriter:OuvrirDocument(sDocument)=Faux ALORS
               		Erreur("L'ouverture du document a échoué.")
               	SINON
               		FenEtat(FEN_LibreOffice_Writer,ActifDevant)
               		
               		//Récupérer l'adresse du répertoire où seront stockés les fichiers
               		sAdresseSauvegarde sont des chaines	= completerep(frepexe) + "Publipostage_"+DateHeureSys()	
               		
               		//Enregistrer et récupérer le chemin du document actif 
               		SI sDocument = "" ALORS
               			sDocument = gclDocumentWriter:EnregistrerSous()
               		FIN
               		
               		//Lancer le publipostage
               		gclDocumentWriter:Publipostage(COMBO_Fichier[COMBO_Fichier], SAI_CheminAnalyse ,sDocument,sAdresseSauvegarde)
               		
               		//Réafficher la fenêtre dessus tout
               		FenEtat(FEN_LibreOffice_Writer,DessusTout)		
               		
               		//Ouvre l'explorateur Windows sur le résultat
               		lanceappli("explorer.exe " + sAdresseSauvegarde)
               		
               		
               
               	FIN
               FIN
               
               
               
